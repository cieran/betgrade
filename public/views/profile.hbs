                </div>
            </div>
            
            <div class="c100 p100 dark orange">
                <span>{{user.profit}}</span>
                <div class="slice">
                    <div class="bar"></div>
                    <div class="fill"></div>
                </div>
            </div>
        </div>
    
</section>
