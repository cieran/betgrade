    <section class="main-body">
        <div class="bet-column-head">
            <div class="col-xs-6 col-md-6">Student Name</div>
            <div class="col-xs-3 col-md-3">Odds to Pass</div>
            <div class="col-xs-3 col-md-3">Odds to Fail</div>
        </div>
        <div class="bet-column-market" ng-controller="ModalCtrl">
            {{# each items}}
            <div class="col-xs-6 col-md-6"><a class="names" href="{{this.filename}}"> {{this.marketname}} </a></div>
            <div class="col-xs-3 col-md-3"><button class="btn btn-block btn-info" ng-click="open()"> {{this.A}} </button></div>
            <div class="col-xs-3 col-md-3"><button class="btn btn-block btn-info" ng-click="open()"> {{this.B}} </button></div>
            {{/each}}
        </div>    
    </section>